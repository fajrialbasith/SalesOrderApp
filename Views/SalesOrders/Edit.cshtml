@model SalesOrderApp.Models.SalesOrderViewModel

<h2>Edit Sales Order</h2>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="ORDER_NO" class="control-label"></label>
        <input asp-for="ORDER_NO" class="form-control" />
        <span asp-validation-for="ORDER_NO" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
